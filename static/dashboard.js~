
window.onload=function(){
    var height = document.documentElement.clientHeight * 0.5;
    var width = document.documentElement.clientWidth * 0.8;

    var svg = d3.select("svg");
    svg.attr('width', width).attr('height',height).append('rect').attr('width',width).attr('height',height).attr('fill','white');
    $('#darrow').on('click',function(){window.location.replace('../../../sites')});
}
